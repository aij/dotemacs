	url = git@github.com:magit/git-modes.git
[submodule "hl-todo"]
	path = lib/hl-todo
	url = git@github.com:tarsius/hl-todo.git
[submodule "libgit"]
	build-step = make
	path = lib/libgit
	url = git@github.com:magit/libegit2.git
[submodule "macrostep"]
	# pending https://github.com/joddie/macrostep/pull/24
	#     and https://github.com/joddie/macrostep/pull/26
	remote = stale git@github.com:joddie/macrostep.git
	path = lib/macrostep
	url = git@github.com:tarsiiformes/macrostep.git
[submodule "magit"]
	no-byte-compile = lisp/magit-libgit.el
	path = lib/magit
